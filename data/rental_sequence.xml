<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Sequence for Rental Projects -->
        <record id="seq_rental_project" model="ir.sequence">
            <field name="name">Rental Project</field>
            <field name="code">rental.project</field>
            <field name="prefix">RENT/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="implementation">standard</field>
        </record>
        
        <!-- Sequence for Equipment Code -->
        <record id="seq_rental_equipment" model="ir.sequence">
            <field name="name">Rental Equipment</field>
            <field name="code">rental.equipment</field>
            <field name="prefix">EQ-</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="implementation">standard</field>
        </record>
        
        <!-- Sequence for Serial Numbers (if auto-generated) -->
        <record id="seq_rental_serial" model="ir.sequence">
            <field name="name">Equipment Serial Number</field>
            <field name="code">rental.equipment.serial</field>
            <field name="prefix">SN-</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="implementation">standard</field>
        </record>
        
    </data>
</odoo>

<!-- Sequences Created:

RENT/00001 - Rental project numbers
EQ-0001 - Equipment codes
SN-000001 - Auto-generated serial numbers 
-->